{
  "triage": "data/outputs/phase1/phase1_triage.xlsx",
  "gsc": "data/outputs/phase2/gsc_queries.csv",
  "expand": true,
  "per_seed": 3,
  "max_queries": 120,
  "must": [
    "candle",
    "soy",
    "scented",
    "handmade"
  ],
  "include": [
    "vanilla",
    "lavender",
    "pumpkin",
    "eucalyptus",
    "gift",
    "bundle",
    "bulk",
    "wax melt",
    "jar",
    "wood wick"
  ],
  "exclude": [
    "amazon",
    "pinterest",
    "reddit",
    "youtube",
    "free",
    "download",
    "pdf",
    "svg",
    "template",
    "clipart",
    "wallpaper"
  ],
  "out": "data/inputs/phase2/keyword_map.csv"
}